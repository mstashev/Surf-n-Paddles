<div class="container">
  <p id="notice"><%= notice %></p>

  <h1>Posts</h1>
  <%= paginate @posts %>


  <% @posts.each do |post| %>
    <% render partial: "posts/post", locals: {post: post} %>
  <% end %>

  <%= render @posts %>

  <%= paginate @posts %>
  <br>
  <div class="col-xs-12">
    <%= link_to 'New Post', new_post_path %>
  </div>
</div>
